{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "info.html", "use": "@vercel/static" },
    { "src": "leaderboard.html", "use": "@vercel/static" },
    { "src": "admin-kyc.html", "use": "@vercel/static" },
    { "src": "admin-prizes.html", "use": "@vercel/static" },
    { "src": "INFO_PAGE_GUIDE.html", "use": "@vercel/static" },
    { "src": "ZIP_SUMMARY.html", "use": "@vercel/static" },
    { "src": "style.css", "use": "@vercel/static" },
    { "src": "leaderboard.css", "use": "@vercel/static" },
    { "src": "script.js", "use": "@vercel/static" },
    { "src": "firebase-config.js", "use": "@vercel/static" },
    { "src": "leaderboard.js", "use": "@vercel/static" },
    { "src": "tonconnect-manifest.json", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/", "dest": "/index.html" },
    { "handle": "filesystem" }
  ]
}
